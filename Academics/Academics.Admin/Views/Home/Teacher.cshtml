@model List<Teacher>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th>
				#
			</th>
			<th>
				Create At
			</th>
			<th>
				Created By
			</th>
			<th>
				FullName
			</th>
			<th>
				Description
			</th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (Teacher item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.CreatedAt</td>
				<td>@item.CreatedBy</td>
				<td>@item.FullName</td>
				<td>@item.Description</td>
				<td>
					<a href="@Url.Action("EditTeacher","Home", new{id=item.Id})">edit</a>
				</td>
				<td>
					<a href="@Url.Action("DeleteTeacher","Home", new{id=item.Id})">delete</a>
				</td>
			</tr>
		}
	</tbody>
</table>

<br />
<hr />
<a href="@Url.Action("EditTeacher","Home")" class="btn btn-default">Add Teacher</a>